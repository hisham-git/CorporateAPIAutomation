          },
          "oca": {
            "id": "/Data/1/oca",
            "type": "integer"
          }
        },
		"required": [
			"ID",
			"Name",
			"Description",
			"IPEDSCode",
			"SortPosition",
			"IsActive",
			"oca"
		]
