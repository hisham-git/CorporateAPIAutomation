{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "",
  "type": "object",
  "properties": {
    "Module": {
      "id": "/Module",
      "type": "string",
	  "enum": [
        "corporate"
      ]
    },
    "Service": {
      "id": "/Service",
      "type": "string",
	  "enum": [
        "reference"
      ]
    },
    "Action": {
      "id": "/Action",
      "type": "string",
	  "enum": [ 
        "getCollection"
      ]
    },
    "Header": {
      "id": "/Header",
      "type": "object",
      "properties": {
        "ReferenceType": {
          "id": "/Header/ReferenceType",
          "type": "string",
		  "enum": [           
            "CountryCode"
          ]
        }
      }
    },
    "Params": {
      "id": "/Params",
      "type": "object",
      "properties": {
        "Description": {
          "id": "/Params/Description",
          "type": "string"
        },
        "IDs": {
          "id": "/Params/IDs",
          "type": "array",
          "items": {
            "id": "/Params/IDs/0",
